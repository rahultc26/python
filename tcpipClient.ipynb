    "import socket \n",
    "\n",
    "s = socket.socket()\n",
    "\n",
    "s.connect((\"localhost\",4000))\n",
    "\n",
    "msg = s.recv(1024)\n",
    "\n",
    "while msg:\n",
    "    print(\"Received :\",msg.decode())\n",
    "    msg = s.recv(1024)"
